generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserProfile {
  id        String   @id @default(cuid())
  clerkId   String   @unique
  email     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
